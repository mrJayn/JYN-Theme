{
	"name": "jyn-theme",
	"displayName": "JYN Theme",
	"description": "A basic theme for VS Code.",
	"publisher": "mjayne",
	"license": "MIT",
	"version": "1.0.3",
	"repository": {
		"type": "git",
		"url": "https://github.com/mrJayn/JYN-Theme"
	},
	"bugs": {
		"url": "https://github.com/mrJayn/JYN-Theme/issues"
	},
	"engines": {
		"vscode": "^1.70.0"
	},
	"scripts": {
		"build": "node build.js",
		"watch": "npm run build && chokidar \"themes/**/*.json5\" \"tokens/**/*.json5\" -c \"npm run build\"",
		"publish": "vsce publish --yarn"
	},
	"galleryBanner": {
		"color": "#27272a",
		"theme": "dark"
	},
	"icon": "/logo.png",
	"categories": [
		"Themes"
	],
	"keywords": [
		"JYN",
		"dark",
		"Style Dictionary"
	],
	"contributes": {
		"themes": [
			{
				"label": "JYN Theme",
				"uiTheme": "vs-dark",
				"path": "./build/jyn.color-theme.json"
			}
		]
	},
	"devDependencies": {
		"@vscode/vsce": "^2.19.0",
		"chokidar-cli": "^3.0.0",
		"style-dictionary": "^3.8.0"
	}
}
